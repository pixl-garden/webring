{
    "builds": [
        {
            "src": "internal/api/*.go",
            "use": "@vercel/go",
            "config": { "goVersion": "1.22" }
        }
    ],
    "routes": [
        { "src": "/", "dest": "/internal/api/index.go" },
        { "src": "/next", "dest": "/internal/api/next.go" },
        { "src": "/prev", "dest": "/internal/api/prev.go" },
        { "src": "/api/members", "dest": "/internal/api/members.go" }
    ]
}
